            "content_enabled": false,
            "content_for": "image",
            "heading_subheading_section_mobile": false,
            "content_horizontal_alignment": "justify-center",
            "content_vertical_alignment": "items-center",
            "heading": "There are three layers to a tooth; the enamel, the dentin, and the nerve chamber.",
            "heading_color": "#000000",
            "heading_color_mobile": "#000000",
            "heading_fontsize": "text-xl",
            "heading_fontsize_mobile": "text-xl",
            "heading_fontweight": "font-normal",
            "subheading": "",
            "subheading_color": "#000000",
            "subheading_color_mobile": "#000000",
            "subheading_fontsize": "text-5xl",
            "subheading_fontsize_mobile": "text-3xl",
            "subheading_fontweight": "font-normal",
            "content": "",
            "content_color": "#000000",
            "content_color_mobile": "#000000",
            "content_fontsize": "text-5xl",
            "content_fontsize_mobile": "text-3xl",
