            "image": "shopify:\/\/shop_images\/Artboard_1_4x_050d1f14-15cc-4fa5-9b9d-9d6f3c5fc4cf.png",
            "image_mobile": "shopify:\/\/shop_images\/Artboard_1_4x_050d1f14-15cc-4fa5-9b9d-9d6f3c5fc4cf.png",
            "image_fit": "stretch",
